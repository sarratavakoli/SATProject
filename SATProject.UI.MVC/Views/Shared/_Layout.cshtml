<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>SAT Scheduling</title>

    <!-- Favicons -->
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Bootstrap core CSS -->
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/style-responsive.css" rel="stylesheet">

    @await RenderSectionAsync("css", false)

 </head> 
 <body>
    <section id="container">
        <!-- **********************************************************************************************************************************************************
            TOP BAR CONTENT & NOTIFICATIONS
            *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>
            <!--logo start-->
            <a href="@Url.Action("Index", "Home")" class="logo"><b>SAT<span>SCHEDULING</span></b></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
            </div>


            <div class="top-menu">

                @*<li><a class="logout" href="login.html">Logout</a></li>*@
                <partial name="_LoginPartial" />


            </div>
        </header>
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
            MAIN SIDEBAR MENU
            *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">

                    <p class="centered"><img src="~/img/logo.png" class="img-circle" width="90%"></p>
                    @if (User.Identity.IsAuthenticated)
                    {
                        <h5 class="centered">Welcome back!</h5>
                    }

                    <li class="">
                        <a href="@Url.Action("Index", "Courses")">
                            <i class="fa fa-list"></i>
                            <span>Courses</span>
                        </a>
                    </li>

                    <li class="">
                        <a href="@Url.Action("Index", "Enrollments")">
                            <i class="fa fa-list"></i>
                            <span>Enrollments</span>
                        </a>
                    </li>

                    <li class="">
                        <a href="@Url.Action("Index", "ScheduledClasses")">
                            <i class="fa fa-list"></i>
                            <span>Classes</span>
                        </a>
                    </li>

                    <li class="">
                        <a href="@Url.Action("Index", "Students")">
                            <i class="fa fa-user"></i>
                            <span>Students</span>
                        </a>
                    </li>


                    @if (User.IsInRole("Admin"))
                    {
                        <li class="sub-menu">
                            <a href="javascript:;">
                                <i class="fa fa-th"></i>
                                <span>Admin</span>
                            </a>
                            <ul class="sub">
                                <li><a href="@Url.Action("ListRoles", "Administration")">Roles</a></li>
                                <li><a href="@Url.Action("ListUsers", "Administration")">Users</a></li>
                                <li><a href="@Url.Action("Index", "ScheduledClassStatus")">Class Catalog Statuses</a></li>
                                <li><a href="@Url.Action("Index", "StudentStatus")">Student Statuses</a></li>

                            </ul>
                        </li>
                    }
                     <li>
            <a href="@Url.Action("Contact", "StronglyTypedData")">
              <i class="fa fa-envelope"></i>
              <span>Contact Form</span>
           
              </a>
          </li>

                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!-- **********************************************************************************************************************************************************
            MAIN CONTENT
            *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
            

                @RenderBody()

            
            
            <!-- /wrapper -->
        </section>
        <!-- /MAIN CONTENT -->
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                <p>
                    Copyright &copy; All Rights Reserved
                </p>
                <div class="credits">
                    @*Created with Dashio template by <a href="https://templatemag.com/">TemplateMag</a>*@
                </div>
                <a href="#" class="go-top">
                    <i class="fa fa-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>


    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="~/lib/jquery.ui.touch-punch.min.js"></script>
    <script class="include" type="text/javascript" src="~/lib/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/lib/jquery.scrollTo.min.js"></script>
    <script src="~/lib/jquery.nicescroll.js" type="text/javascript"></script>
    <!--common script for all pages-->
    <script src="~/lib/common-scripts.js"></script>
    <!--script for this page-->
    
    @await RenderSectionAsync("Scripts", false)

</body>

</html>
